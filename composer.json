{
	"name" : "amylian/yii2-amylian-doctrine-common",
	"description" : "ATTENTION EXPERIMENTAL! Common components for yii2-amylian-doctrine-*",
	"type" : "yii2-extension",
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"authors" : [{
			"name" : "Andreas Prucha (Abexto - Helicon Software Development - Andreas Prucha / Amylian Project)",
			"email" : "andreas.prucha@gmail.com",
			"homepage" : "http://www.abexto.com",
			"role" : "Developer"
		}
	],
	"repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}
	],
	"require" : {
		"doctrine/common" : "*@stable",
		"amylian/doctrine-common" : "dev-master",
		"amylian/yii2-amylian-doctrine-base" : "dev-master"
	},
	"require-dev" : {
		"amylian/yii2-amylian-phpunit" : "*",
		"phpunit/phpunit" : "*"
	},
	"autoload" : {
		"psr-4" : {
			"amylian\\yii\\doctrine\\common\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"amylian\\yii\\doctrine\\common\\tests\\" : "tests/"
		}
	},
	"extra" : {
		"bootstrap" : "amylian\\yii\\doctrine\\common\\PackageBootstrap"
	}
}